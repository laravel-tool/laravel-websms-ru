{
    "name": "laravel-tool/laravel-websms-ru",
    "description": "Laravel WebSMS.ru channel provider",
    "type": "laravel-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Potemkin",
            "email": "ysoft2000@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "illuminate/http": "^9.0",
        "illuminate/notifications": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTool\\LaravelWebsmsRu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelTool\\LaravelWebsmsRu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTool\\LaravelWebsmsRu\\WebsmsServiceProvider"
            ]
        }
    }
}
